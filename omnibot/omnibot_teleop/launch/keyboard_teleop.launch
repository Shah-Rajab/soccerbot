<launch>
  <!-- omnibot_teleop_key already has its own built in velocity smoother -->
  <node pkg="omnibot_teleop" type="omnibot_teleop_key" name="omnibot_teleop_keyboard"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.5" type="double"/>
    <remap from="omnibot_teleop_keyboard/cmd_vel" to="/omnibot/cmd_vel"/>
  </node>
</launch>
